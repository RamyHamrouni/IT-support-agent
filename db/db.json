  "kb": [
    {
      "id": "KB-001",
      "category": "Printer",
      "issue_code": "PR-001",
      "question": "Printer shows 'Paper Jam' error",
      "answer": "If your HP printer shows a 'Paper Jam' error, follow these steps:\n1. Power off printer and unplug.\n2. Open all covers and remove jammed paper carefully.\n3. Inspect rollers and paper paths for debris.\n4. Close covers and power on.\n5. Print a test page to confirm resolution.\nFor more info: Click here.",
      "tags": [
        "printer",
        "paper jam"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Printers-Knowledge-Base/Paper-Jam/td-p/12345"
    },
    {
      "id": "KB-002",
      "category": "Printer",
      "issue_code": "PR-002",
      "question": "Printer not connecting to Wi-Fi",
      "answer": "If your printer cannot connect to Wi-Fi:\n1. Ensure printer and router are powered on and in range.\n2. Restart both devices.\n3. Reconnect printer using wireless setup.\n4. Confirm correct Wi-Fi password.\n5. Check firewall/antivirus.\n6. Print a network configuration page.\nFor details: Click here.",
      "tags": [
        "printer",
        "wifi",
        "network"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Printers-Knowledge-Base/WiFi-Setup/td-p/67890"
    },
    {
      "id": "KB-003",
      "category": "Windows",
      "issue_code": "WIN-001",
      "question": "Cannot install HP driver on Windows 11",
      "answer": "To install an HP driver on Windows 11:\n1. Download latest driver from HP Support.\n2. Right-click and 'Run as administrator'.\n3. Temporarily disable antivirus.\n4. Follow prompts.\n5. Restart computer.\n6. Verify driver in Device Manager.\nMore info: Click here.",
      "tags": [
        "windows",
        "driver",
        "installation"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Windows-11-Drivers/Driver-Installation-Issues/td-p/13579"
    },
    {
      "id": "KB-004",
      "category": "Network",
      "issue_code": "NET-001",
      "question": "VPN disconnects frequently",
      "answer": "If VPN disconnects frequently:\n1. Restart VPN client and computer.\n2. Check internet connection.\n3. Update VPN client.\n4. Verify firewall settings.\n5. Try different network/server.\nFor details: Click here.",
      "tags": [
        "vpn",
        "network",
        "disconnect"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Network-Support/VPN-Troubleshooting/td-p/24680"
    },
    {
      "id": "KB-005",
      "category": "Account",
      "issue_code": "ACC-001",
      "question": "Forgot HP account password",
      "answer": "To reset HP account password:\n1. Visit https://account.hp.com.\n2. Click 'Forgot Password'.\n3. Enter registered email.\n4. Follow instructions to reset.\n5. Log in with new password.\nMore info: Click here.",
      "tags": [
        "account",
        "password",
        "login"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Account-Support/Password-Reset/td-p/11223"
    },
    {
      "id": "KB-006",
      "category": "Software",
      "issue_code": "SW-001",
      "question": "Application crashes on launch",
      "answer": "If an HP application crashes on launch:\n1. Restart computer.\n2. Ensure application updated.\n3. Reinstall application.\n4. Check system updates.\n5. Temporarily disable antivirus.\n6. Check for conflicts.\nMore info: Click here.",
      "tags": [
        "software",
        "crash"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Software-FAQ/App-Crash/td-p/33456"
    },
    {
      "id": "KB-007",
      "category": "Printer",
      "issue_code": "PR-003",
      "question": "Printer prints blank pages",
      "answer": "If printer prints blank pages:\n1. Check toner/ink levels.\n2. Ensure cartridges installed properly.\n3. Run cleaning cycle.\n4. Verify print settings.\n5. Restart printer.\nMore info: Click here.",
      "tags": [
        "printer",
        "ink",
        "toner"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Printers-Knowledge-Base/Blank-Pages/td-p/44567"
    },
    {
      "id": "KB-008",
      "category": "Network",
      "issue_code": "NET-002",
      "question": "Cannot access internal network drives",
      "answer": "If you cannot access internal network drives:\n1. Check VPN connection.\n2. Verify permissions.\n3. Restart computer.\n4. Test on another device.\nMore info: Click here.",
      "tags": [
        "network",
        "vpn"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Network-Support/Internal-Drives/td-p/55678"
    },
    {
      "id": "KB-009",
      "category": "Windows",
      "issue_code": "WIN-002",
      "question": "Blue screen error after update",
      "answer": "If BSOD after update:\n1. Boot in Safe Mode.\n2. Uninstall recent updates.\n3. Run system file checker.\n4. Check driver updates.\n5. System restore if necessary.\nMore info: Click here.",
      "tags": [
        "windows",
        "bsod"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Windows-11-Drivers/BSOD/td-p/66789"
    },
    {
      "id": "KB-010",
      "category": "Account",
      "issue_code": "ACC-002",
      "question": "Cannot login to HP account",
      "answer": "If you cannot login:\n1. Reset password.\n2. Clear browser cache.\n3. Try different browser.\n4. Check verification email.\nMore info: Click here.",
      "tags": [
        "account",
        "login"
      ],
      "source_url": "https://h30434.www3.hp.com/t5/Account-Support/Login-Issues/td-p/77890"
